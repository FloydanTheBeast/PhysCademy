    "concurrently": "^3.6.0",
    "css-loader": "^1.0.0",
    "devtron": "^1.4.0",
    "electron": "^15.5.5",
    "electron-builder": "^20.28.1",
    "electron-debug": "^2.0.0",
    "electron-react-devtools": "^0.5.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "svg-inline-loader": "^0.8.0",
